{
  "name": "renku-ui-server",
  "version": "0.11.13",
  "description": "Server for renku API",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon dist/index.js",
    "start:dev-debug": "nodemon --inspect dist/index.js",
    "start:dev-debug-brk": "nodemon --inspect-brk dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "dev-debug": "concurrently \"npm:build:dev\" \"npm:start:dev-debug\"",
    "dev-debug-brk": "concurrently \"npm:build:dev\" \"npm:start:dev-debug-brk\""
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "winston": "^3.3.3",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.4"
  }
}