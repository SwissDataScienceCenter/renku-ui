{
  "name": "renku-ui-server",
  "version": "4.8.0",
  "description": "Server for renku API",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "coverage": "jest --coverage",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "dev-debug": "concurrently \"npm:build:dev\" \"npm:start:dev-debug\"",
    "dev-debug-brk": "concurrently \"npm:build:dev\" \"npm:start:dev-debug-brk\"",
    "lint": "eslint --max-warnings=0 src/",
    "format": "prettier -w .",
    "format-check": "prettier -c .",
    "format-changed": "pretty-quick --pattern \"server/**/*.*\"",
    "format-commit": "pretty-quick --staged --pattern \"server/**/*.*\"",
    "prestart": "npm run build",
    "start": "node .",
    "start:dev": "nodemon dist/src/index.js",
    "start:dev-debug": "nodemon --inspect dist/src/index.js",
    "start:dev-debug-brk": "nodemon --inspect-brk dist/src/index.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@sentry/node": "^7.119.2",
    "cross-fetch": "^3.1.8",
    "express": "^4.21.2",
    "express-prom-bundle": "^6.6.0",
    "http-proxy-middleware": "^2.0.9",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.1",
    "openid-client": "^4.9.1",
    "uuid": "^8.3.2",
    "winston": "^3.10.0",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.27.2",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.2",
    "@types/node": "^20.11.7",
    "@types/uuid": "^8.3.1",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.8.0",
    "ioredis-mock": "^5.6.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.3.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.2.4"
  }
}
