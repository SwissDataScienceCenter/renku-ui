{
  "name": "renku-ui-server",
  "version": "2.0.0",
  "description": "Server for renku API",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "coverage": "jest --coverage",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "dev-debug": "concurrently \"npm:build:dev\" \"npm:start:dev-debug\"",
    "dev-debug-brk": "concurrently \"npm:build:dev\" \"npm:start:dev-debug-brk\"",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "start": "node .",
    "start:dev": "nodemon dist/src/index.js",
    "start:dev-debug": "nodemon --inspect dist/src/index.js",
    "start:dev-debug-brk": "nodemon --inspect-brk dist/src/index.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@sentry/node": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.1.4",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "openid-client": "^4.9.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.27.2",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.17.21",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.24.0",
    "ioredis-mock": "^5.6.0",
    "jest": "^27.2.4",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.0.5",
    "typescript": "^4.2.4"
  }
}
