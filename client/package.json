  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles --watch --recursive",
    "start-js": "REACT_APP_UI_DEV_MODE=$UI_DEV_MODE REACT_APP_RENKU_UI_URL=$RENKU_UI_URL craco --max_old_space_size=4096 start",
    "start": "npm-run-all -p watch-css start-js ",
    "build": "npm run build-css && REACT_APP_UI_DEV_MODE=false craco --max_old_space_size=4096 build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "node_modules/eslint/bin/eslint.js --max-warnings=0 src/",
    "lint-fix": "node_modules/eslint/bin/eslint.js src/ --fix",
    "node": "node_modules/.bin/babel-node"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@craco/craco": "^5.8.0",
    "cz-conventional-changelog": "^3.1.0",
