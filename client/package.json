    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "^node_modules/(?!(highlight.js|katex|react-markdown|react-pdf)).*/"
    ]
