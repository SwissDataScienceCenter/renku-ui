{
  "name": "renku-ui",
  "version": "3.16.0",
  "private": true,
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "tsc && vite build",
    "vite:build:yolo": "vite build",
    "vite:preview": "vite preview",
    "vite:test": "vitest",
    "start-js": "BROWSER=none REACT_APP_UI_DEV_MODE=$UI_DEV_MODE REACT_APP_RENKU_UI_URL=$RENKU_UI_URL craco --max_old_space_size=4096 start",
    "start": "react-scripts start",
    "build": "REACT_APP_UI_DEV_MODE=false craco --max_old_space_size=4096 build",
    "build:analyze": "REACT_APP_UI_DEV_MODE=false craco --max_old_space_size=4096 build --analyze",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint --max-warnings=0 src/",
    "lint-fix": "eslint src/ --fix",
    "format": "prettier -w .",
    "format-check": "prettier -c .",
    "format-changed": "pretty-quick --pattern \"client/**/*.*\"",
    "format-commit": "pretty-quick --staged --pattern \"client/**/*.*\"",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build --quiet",
    "storybook-start-server": "http-server storybook-static --port 6006 --silent",
    "storybook-wait-server": "wait-on http://127.0.0.1:6006",
    "storybook-test": "test-storybook",
    "storybook-compile-and-test": "concurrently -k -s first -n 'BUILD,TEST' -c 'magenta,blue' 'npm run storybook-build && npm run storybook-start-server' 'npm run storybook-wait-server && npm run storybook-test'"
  },
  "type": "module",
  "dependencies": {
    "@ckeditor/ckeditor5-react": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@renku/ckeditor5-build-renku": "^0.0.6",
    "@sentry/react": "^7.81.1",
    "bootstrap": "^5.3.2",
    "d3": "^7.8.5",
    "dagre-d3": "^0.6.4",
    "dompurify": "^2.4.7",
    "dropzone": "^6.0.0-beta.2",
    "file-saver": "^2.0.5",
    "filesize": "^10.1.0",
    "highlight.js": "^11.9.0",
    "immutability-helper": "^3.1.1",
    "jquery": "^3.7.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "mermaid": "^10.6.1",
    "pica": "^9.0.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-autosuggest": "^10.1.0",
    "react-avatar-editor": "^13.0.0",
    "react-bootstrap-icons": "^1.10.3",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.48.2",
    "react-ipynb-renderer": "^2.1.3",
    "react-js-pagination": "^3.0.3",
    "react-masonry-css": "^1.0.16",
    "react-media": "^1.10.0",
    "react-pdf": "^7.5.1",
    "react-redux": "^7.2.9",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "reactstrap": "^9.2.1",
    "sass": "^1.69.5",
    "showdown": "^2.1.0",
    "showdown-highlight": "^2.1.8",
    "showdown-katex": "^0.6.0",
    "showdown-mermaid": "^0.0.1",
    "swiper": "^9.4.1",
    "xregexp": "^5.1.1"
  },
  "devDependencies": {
    "@types/dompurify": "^2.4.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest-websocket-mock": "^2.5.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "overrides": {
    "ansi-to-react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "showdown-katex": {
      "showdown": "^2.1.0"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
